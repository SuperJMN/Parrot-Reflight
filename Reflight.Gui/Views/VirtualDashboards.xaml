    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:db="using:Reflight.Gui.Views.Dashboards">

    <DataTemplate x:Key="Standard">
        <db:StandardDashboard />
    </DataTemplate>

    <DataTemplate x:Key="Original">
        <db:OriginalDashboard />
    </DataTemplate>

    <DataTemplate x:Key="Original with graphs">
